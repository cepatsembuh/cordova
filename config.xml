<?xml version='1.0' encoding='utf-8'?>
<widget id="id.cepatsembuh" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CepatSembuh</name>
    <description>
      An app to help people to find the nearest faskes in their location
    </description>
    <author>
      CepatSembuh Team
    </author>
    <content src="www/index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="https://cepatsembuh.firebaseio.com" />
    <preference name="Orientation" value="portrait" />
    <preference name="phonegap-version" value="cli-6.3.0" />
    <platform name="android">
        <icon src="res/icon.png" density="mdpi"/>
        <icon src="res/icon.png" density="hdpi"/>
        <icon src="res/icon.png" density="xhdpi"/>
        <icon src="res/icon.png" density="xxhdpi"/>
        <icon src="res/icon.png" density="xxxhdpi"/>
    </platform>
</widget>
